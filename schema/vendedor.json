{
  "$id": "http://example.com/example.json",
  "type": "object",
  "restricted": ["ownerId"],
  "title": "Vendedor",
  "key": "vendedor",
  "secure": false,
  "namespace": "venta",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id",
      "default": 0,
      "width": 50,
      "filter": "eq",
      "editable": true,
      "examples": [1]
    },
    "comisiona": {
      "$id": "/properties/comisiona",
      "type": "boolean",
      "title": "comisiona",
      "editable": true,
      "filterable": true,
      "filterRenderer": "BooleanRenderer",
      "formmatter": "BooleanFormatter"
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "Nombre",
      "default": "",
      "filter": "LIKE",
      "editable": true,
      "filterable": true,
      "examples": ["any name"],
      "width": 250
    },
    "ownerId": {
      "$id": "/properties/ownerId",
      "type": "integer",
      "title": "Usuario",
      "table": "usuario",
      "filterable": true,
      "route": "login/usuario/findLikeName",
      "metadataType": "usuario",
      "element": "autocomplete",
      "elementOptions": {
        "primary": "name"
      },
      "width": 250
    }
  },
  "required": ["name", "ownerId"],
  "actions": ["Crear", "Editar"],
  "belongsTo": [],
  "layout": [
    {
      "title": "Grupo",
      "cols": 8,
      "fields": ["name", "ownerId", "comisiona"]
    }
  ],
  "table": {
    "sum": [],
    "fields": ["name", "ownerId", "comisiona"]
  }
}
