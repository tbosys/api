    },
    "updatedAt": {
      "$id": "/properties/updatedAt",
      "type": "string",
      "title": "Actualizado"
    }
  },
  "required": ["name", "productoDepartamentoId"],
  "belongsTo": ["productoDepartamento"],
  "actions": ["Crear", "Editar", "Borrar"],
  "layout": [
    {
      "title": "Informaci√≥n",
