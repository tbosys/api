{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Comisiones",
  "key": "comisionHistorico",
  "limit": 1000,
  "maxLimit": 20000,
  "shareLevel": 5,
  "namespace": "facturacion",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id",
      "width": 50
    },
    "monto": {
      "$id": "/properties/monto",
      "type": "number",
      "title": "Comision"
    },
    "tipo": {
      "$id": "/properties/tipo",
      "type": "string",
      "title": "tipo",
      "filterable": true,
      "filterRenderer": "SelectFilter",

      "sortable": true,
      "enum": ["FA", "NC", "ND", "PA-", "PA+"]
    },
    "fecha": {
      "$id": "/properties/fecha",
      "type": "string",
      "title": "fecha",
      "filterable": true,
      "sortable": true,
      "filterRenderer": "DateFilter"
    },
    "movimientoInventarioId": {
      "$id": "/properties/movimientoInventarioId",
      "type": "integer",
      "title": "movimientoInventario",
      "table": "movimientoInventario",
      "metadataType": "movimientoInventario",
      "filterable": true,
      "route": "movimientoInventario/findLikeName",
      "element": "autocomplete",
      "width": 350,
      "elementOptions": {
        "primary": "name"
      }
    },

    "lineaPagoDocumentoId": {
      "$id": "/properties/lineaPagoDocumentoId",
      "type": "integer",
      "title": "lineaPagoDocumento",
      "table": "lineaPagoDocumento",
      "metadataType": "lineaPagoDocumento",
      "filterable": true,
      "route": "lineaPagoDocumento/findLikeName",
      "element": "autocomplete",
      "width": 350,
      "elementOptions": {
        "primary": "pagoDocumentoId"
      }
    },

    "productoId": {
      "$id": "/properties/productoId",
      "type": "string",
      "title": "Producto",
      "filterable": true,
      "sortable": true,
      "select": "producto.name",
      "width": 350
    },

    "ownerId": {
      "$id": "/properties/ownerId",
      "type": "integer",
      "title": "Propietario",
      "table": "owner",
      "filterable": true,
      "sortable": true,
      "route": "owner/findLikeName",
      "metadataType": "owner",
      "element": "autocomplete",
      "elementOptions": {
        "primary": "name"
      },
      "width": 250
    },
    "createdBy": {
      "$id": "/properties/createdBy",
      "type": "string",
      "title": "Creado por"
    },
    "updatedBy": {
      "$id": "/properties/updatedBy",
      "type": "string",
      "title": "Actualizado por"
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado",
      "filterable": true,
      "filterRenderer": "DateFilter"
    },
    "updatedAt": {
      "$id": "/properties/updatedAt",
      "type": "string",
      "title": "Actualizado"
    }
  },
  "belongsTo": ["movimientoInventario", "movimientoInventario>producto", "owner"],
  "required": [],
  "table": {
    "sum": ["monto"],
    "fields": ["id", "ownerId", "productoId", "monto", "tipo", "fecha"]
  }
}
