{
  "$id": "http://example.com/example.json",
  "type": "object",
  "restricted": [],
  "limit": 100,
  "title": "Boletas de Movimientos de inventario",
  "key": "boleta",
  "namespace": "inventario",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "estado": {
      "$id": "/properties/estado",
      "type": "string",
      "title": "Estado",
      "element": "select",
      "readOnly": true,
      "filterable": true,
      "filterRenderer": "SelectFilter",
      "enum": ["por costear", "por aceptar", "por aprobar", "por aplicar", "archivado"],
      "width": 110
    },
    "tipo": {
      "$id": "/properties/tipo",
      "element": "select",
      "enum": ["EN", "SA", "CO", "NC", "ND", "DE"],
      "type": "string",
      "title": "Tipo",
      "filterable": true,
      "filterRenderer": "SelectFilter"
    },
    "referencia": {
      "$id": "/properties/referencia",
      "type": "string",
      "title": "Referencia",
      "filterable": true
    },
    "descripcion": {
      "$id": "/properties/descripcion",
      "type": "string",
      "element": "textArea",
      "title": "Descripcion",
      "width": 350,
      "formatter": "TextFormatter"
    },

    "createdBy": {
      "$id": "/properties/createdBy",
      "type": "string",
      "title": "Creado por"
    },
    "updatedBy": {
      "$id": "/properties/updatedBy",
      "type": "string",
      "title": "Actualizado por",
      "width": 200
    },
    "fecha": {
      "$id": "/properties/documentoFecha",
      "type": "string",
      "title": "Fecha",
      "filterable": true,
      "filterRenderer": "DateFilter"
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado"
    },
    "updatedAt": {
      "$id": "/properties/updatedAt",
      "type": "string",
      "title": "Actualizado"
    },
    "movimientoInventario": {
      "isJSON": true,
      "width": 100,
      "title": "Productos",
      "formatter": "ChildFormatter",
      "fields": ["__productoId", "cantidad", "costo"],
      "editables": ["cantidad", "costo"],
      "idField": "productoId",
      "element": "table",
      "type": "string",
      "items": {
        "type": "object"
      }
    }
  },
  "belongsTo": [],
  "required": ["referencia"],
  "actions": ["Ver", "Crear", "Editar", "Aprobar", "Aplicar", "Borrar"],
  "layout": [
    {
      "title": "Informaci√≥n",
      "cols": 12,
      "fields": ["referencia", "tipo", "descripcion"]
    },
    {
      "title": "Productos",
      "cols": 12,
      "fieldSize": 12,
      "fields": ["movimientoInventario"]
    }
  ],
  "table": {
    "sum": [],
    "fields": ["id", "referencia", "fecha", "tipo", "estado", "descripcion", "updatedBy"]
  },
  "batch": ["fecha", "estado"]
}
