{
    "$id": "http://example.com/example.json",
    "type": "object",
    "key": "mayorizacion",
    "title": "Mayorizacion",
    "namespace": "contabilidad",
    "restricted": [],
    "properties": {
        "id": {
            "$id": "/properties/id",
            "type": "integer",
            "title": "Id",
            "default": 0
        },
        "name": {
            "$id": "/properties/name",
            "type": "string",
            "title": "Nombre",
            "sortable": true,
            "filterable": true
        },
        "descripcion": {
            "$id": "/properties/descripcion",
            "type": "string",
            "title": "Descripcion",
            "width": 350,
            "sortable": true
        },
        "isDraft": {
            "$id": "/properties/isDraft",
            "type": "boolean",
            "title": "Pendientes",
            "filterRenderer": "BooleanFilter",
            "filterable": true,
            "formatter": "BooleanFormatter"
        },
        "fecha": {
            "$id": "/properties/fecha",
            "type": "string",
            "title": "Fecha",
            "sortable": true,
            "filterable": true,
            "filterRenderer": "DateFilter",
            "select": "journal.fecha"
        },
        "debito": {
            "$id": "/properties/debito",
            "type": "number",
            "title": "Debe"
        },
        "credito": {
            "$id": "/properties/credito",
            "type": "number",
            "title": "Haber"
        },
        "accountName": {
            "$id": "/properties/accountName",
            "type": "string",
            "title": "Cuenta",
            "filterable": true,
            "sortable": true,
            "select": "account.name",
            "width": 300
        },
        "accountCodigo": {
            "$id": "/properties/accountCodigo",
            "type": "string",
            "title": "Cuenta Codigo",
            "filterable": true,
            "sortable": true,
            "select": "account.codigo"
        }
    },
    "required": [],
    "belongsTo": ["journal", "account"],
    "actions": [],
    "layout": [],
    "table": {
        "sum": ["debito","credito"],
        "fields": [
            "id",
            "name",
            "isDraft",
            "accountCodigo",
            "accountName",
            "fecha",
            "descripcion",
            "debito",
            "credito"

        ]
    }
}