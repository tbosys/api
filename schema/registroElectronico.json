{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Registro Electronico",
  "namespace": "monitoreo",
  "key": "registroElectronico",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "clienteId": {
      "$id": "/properties/clienteId",
      "type": "integer",
      "title": "Cliente",
      "table": "cliente",
      "metadataType": "cliente",
      "filterable": true,
      "formatter": "ClienteFormatter",
      "width": 200,
      "route": "cliente/findLikeName",
      "element": "autocomplete",
      "elementOptions": {
        "primary": "name"
      }
    },
    "completo": {
      "$id": "/properties/completo",
      "type": "boolean",
      "title": "completo",
      "default": true,
      "formatter": "BooleanFormatter"
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Fecha",
      "default": true,
      "formatter": "DateTimeFormatter",
      "filterRenderer": "DateTimeFilter",
      "filterable": true,
      "sortable": true
    },
    "environment": {
      "$id": "/properties/environment",
      "type": "string",
      "title": "environment",
      "width": 300,
      "filterable": true
    },
    "tipo": {
      "$id": "/properties/tipo",
      "type": "string",
      "title": "tipo",
      "filterable": true,
      "filterRenderer": "SelectFilter",
      "enum": ["FacturaElectronica", "NotaCreditoElectronica", "NotaDebitoElectronica"],
      "width": 200,
      "sortable": true
    },
    "pdf": {
      "$id": "/properties/pdf",
      "type": "string",
      "title": "PDF",
      "formatter": "LinkFormatter",
      "width": 245
    },
    "xml": {
      "$id": "/properties/xml",
      "type": "string",
      "title": "xml",
      "formatter": "LinkFormatter",
      "width": 245
    },
    "respuestaXml": {
      "$id": "/properties/pdf",
      "type": "string",
      "title": "respuestaXML",
      "formatter": "LinkFormatter",
      "width": 245
    },
    "consecutivo": {
      "$id": "/properties/consecutivo",
      "type": "string",
      "title": "consecutivo",
      "width": 125,
      "sortable": true,
      "filterable": true
    },
    "estado": {
      "$id": "/properties/estado",
      "type": "string",
      "title": "estado",
      "enum": [
        "por firmar",
        "por enviar",
        "por validar",
        "por revisar",
        "por anular",
        "por archivar",
        "archivado"
      ],
      "sortable": true,
      "filterable": true
    },
    "clave": {
      "$id": "/properties/clave",
      "type": "string",
      "title": "clave",
      "sortable": true,
      "filterable": true
    },
    "tipoId": {
      "$id": "/properties/tipoId",
      "type": "integer",
      "title": "tipoId"
    },
    "respuesta": {
      "$id": "/properties/respuesta",
      "type": "string",
      "title": "respuesta",
      "width": 400
    }
  },
  "required": ["consecutivo"],
  "belongsTo": ["cliente"],
  "actions": ["Reintentar"],
  "layout": [
    {
      "title": "Informaci√≥n",
      "fields": []
    }
  ],
  "table": {
    "sum": [],
    "fields": [
      "clienteId",
      "estado",
      "tipo",
      "createdAt",
      "consecutivo",
      "clave",
      "pdf",
      "xml",
      "respuestaXml",
      "tipoId",
      "respuesta"
    ]
  }
}
