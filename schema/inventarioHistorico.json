{
  "$id": "http://example.com/example.json",
  "type": "object",
  "limit": 1000,
  "maxLimit": 5000,
  "key": "inventarioHistorico",
  "title": "Inventario Historico",
  "namespace": "auditoria",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "inventario": {
      "$id": "/properties/inventario",
      "type": "number",
      "title": "Inventario",
      "sortable": true
    },
    "costo": {
      "$id": "/properties/costo",
      "type": "number",
      "title": "Costo",
      "sortable": true
    },
    "valor": {
      "$id": "/properties/valor",
      "type": "number",
      "title": "Valor",
      "select": "(inventarioHistorico.costo*inventarioHistorico.inventario)",

      "sortable": true
    },

    "productoId": {
      "$id": "/properties/producto",
      "type": "integer",
      "title": "Producto",
      "table": "producto",
      "metadataType": "producto",
      "filterable": true,
      "sortable": true,
      "width": 250,
      "route": "producto/findLikeName",
      "element": "autocomplete",
      "elementOptions": {
        "primary": "name"
      }
    },
    "familia": {
      "$id": "/properties/familia",
      "type": "string",
      "title": "Familia",
      "filterable": true,
      "sortable": true,
      "select": "producto.familia",
      "width": 100
    },
    "subFamilia": {
      "$id": "/properties/subFamilia",
      "type": "string",
      "title": "Sub Familia",
      "filterable": true,
      "sortable": true,
      "select": "producto.subFamilia",
      "width": 190
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado",
      "filterable": true,
      "sortable": true,
      "filterRenderer": "DateTimeFilter",
      "width": 150
    }
  },
  "belongsTo": ["producto"],
  "required": ["cantidad"],
  "actions": [],
  "table": {
    "sum": ["valor"],
    "fields": ["id", "productoId", "createdAt", "inventario", "costo", "valor", "familia", "subFamilia"]
  },
  "layout": []
}
