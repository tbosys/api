{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Gasto",
  "limit": 100,
  "maxLimit": 500,
  "key": "gasto",
  "namespace": "cxp",
  "restricted": [],
  "multiRowAction": true,
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "estado": {
      "$id": "/properties/estado",
      "type": "string",
      "title": "Estado",
      "filterable": true,
      "default": "por categorizar",
      "filterRenderer": "SelectFilter",
      "enum": ["por categorizar", "por pagar", "archivado"]
    },
    "referencia": {
      "$id": "/properties/referencia",
      "type": "string",
      "title": "referencia"
    },
    "linkRespuesta": {
      "$id": "/properties/linkRespuesta",
      "type": "string",
      "title": "linkRespuesta",
      "width": 240,
      "formatter": "LinkFormatter",
      "prefix": "http://emails.efactura.io/"
    },
    "linkFactura": {
      "$id": "/properties/linkFactura",
      "type": "string",
      "title": "linkFactura",
      "width": 240,
      "formatter": "LinkFormatter"
    },
    "linkFacturaXml": {
      "$id": "/properties/linkFacturaXml",
      "type": "string",
      "title": "linkFacturaXml",
      "width": 240,
      "formatter": "LinkFormatter",
      "prefix": "http://emails.efactura.io/"
    },
    "detalleServicio": {
      "$id": "/properties/detalleServicio",
      "type": "string",
      "title": "detalleServicio"
    },
    "respuestaXML": {
      "$id": "/properties/respuestaXML",
      "type": "string",
      "title": "respuestaXML"
    },
    "resumenFactura": {
      "$id": "/properties/resumenFactura",
      "type": "string",
      "title": "resumenFactura"
    },
    "registroRecibidoId": {
      "$id": "/properties/registroRecibidoId",
      "type": "number",
      "title": "registroRecibidoId"
    },
    "emailId": {
      "$id": "/properties/emailId",
      "type": "string",
      "title": "emailId",
      "width": 240,
      "formatter": "LinkFormatter",
      "prefix": "http://emails.efactura.io/emails/"
    },
    "clave": {
      "$id": "/properties/clave",
      "type": "string",
      "title": "clave",
      "width": 240
    },
    "tipo": {
      "$id": "/properties/tipo",
      "type": "string",
      "title": "Tipo",
      "element": "select",
      "filterRenderer": "SelectFilter",
      "filterable": true,
      "default": "FA",
      "enum": ["FA", "ND", "NC"]
    },
    "descripcion": {
      "$id": "/properties/descripcion",
      "type": "string",
      "title": "descripcion",
      "width": 220
    },
    "fecha": {
      "$id": "/properties/fecha",
      "type": "string",
      "title": "fecha",
      "element": "date"
    },
    "moneda": {
      "$id": "/properties/moneda",
      "type": "string",
      "title": "moneda",
      "enum": ["CRC", "USD"],
      "element": "select",
      "filterRenderer": "SelectFilter",
      "filterable": true,
      "default": "CRC"
    },
    "tipoCambio": {
      "$id": "/properties/tipoCambio",
      "type": "number",
      "title": "tipoCambio"
    },
    "subTotal": {
      "$id": "/properties/subTotal",
      "type": "number",
      "title": "subTotal"
    },
    "impuesto": {
      "$id": "/properties/impuesto",
      "type": "number",
      "title": "impuesto"
    },
    "descuento": {
      "$id": "/properties/descuento",
      "type": "number",
      "title": "descuento"
    },
    "total": {
      "$id": "/properties/total",
      "type": "number",
      "title": "total"
    },
    "saldo": {
      "$id": "/properties/saldo",
      "type": "number",
      "title": "saldo"
    },
    "asignacion": {
      "$id": "/properties/asignacion",
      "type": "string",
      "title": "asignacion"
    },
    "registrarImpuesto": {
      "$id": "/properties/registrarImpuesto",
      "type": "boolean",
      "title": "registrarImpuesto"
    },
    "attachments": {
      "$id": "/properties/attachments",
      "type": "string",
      "title": "Attachments",
      "isJSON": true,
      "formatter": "FileListFormatter",
      "width": 160,
      "element": "multiFileUpLoader"
    },
    "proveedorId": {
      "$id": "/properties/proveedorId",
      "type": "integer",
      "title": "Proveedor",
      "table": "proveedor",
      "route": "proveedor/findLikeName",
      "metadataType": "proveedor",
      "element": "autocomplete",
      "filterable": true,
      "elementOptions": {
        "primary": "name"
      },
      "width": 200,
      "locked": true
    }
  },
  "belongsTo": ["proveedor"],
  "actions": [
    "Crear",
    {
      "title": "Prepagado",
      "type": "modal",
      "size": "xl"
    }
  ],
  "layout": [
    {
      "title": "Información",
      "cols": 4,
      "fields": ["proveedorId", "moneda", "tipoCambio", "subTotal", "impuesto", "descuento", "total", "saldo"]
    },
    {
      "title": "Información",
      "cols": 4,
      "fields": ["referencia", "tipo", "descripcion", "fecha", "attachments"]
    }
  ],
  "table": {
    "sum": [],
    "fields": [
      "proveedorId",
      "estado",
      "descripcion",
      "tipo",
      "fecha",
      "moneda",
      "tipoCambio",
      "subTotal",
      "impuesto",
      "descuento",
      "total",
      "saldo",
      "clave",
      "attachments",
      "linkFactura",
      "detalleServicio",
      "linkFacturaXml",
      "linkRespuesta",
      "respuestaXML"
    ]
  }
}
