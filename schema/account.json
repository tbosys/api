{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Cuenta",
  "limit": 300,
  "maxLimit": 500,
  "key": "account",
  "namespace": "contabilidad",
  "sort": "codigo",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "Nombre",
      "filterable": true,
      "width": 300,
      "locked": true
    },
    "codigo": {
      "$id": "/properties/codigo",
      "type": "string",
      "title": "Codigo",
      "filterable": true,
      "sortable": true,
      "locked": true
    },
    "normalDebit": {
      "$id": "/properties/normalDebit",
      "type": "boolean",
      "formatter": "BooleanFormatter",
      "title": "Debito Normal",
      "filterable": true,
      "sortable": true,
      "width": 140
    },
    "normalCredit": {
      "$id": "/properties/normalCredit",
      "type": "boolean",
      "formatter": "BooleanFormatter",
      "title": "Credito Normal",
      "filterable": true,
      "sortable": true,
      "width": 140
    },
    "balance": {
      "$id": "/properties/balance",
      "type": "number",
      "title": "Balance",
      "filterable": true,
      "sortable": true,
      "editable": true,
      "width": 200
    },
    "balancePendiente": {
      "$id": "/properties/balancePendiente",
      "type": "number",
      "title": "Balance Pendiente",
      "filterable": true,
      "sortable": true,
      "editable": true,
      "width": 200
    },
    "nivel": {
      "$id": "/properties/nivel",
      "type": "integer",
      "title": "Nivel",
      "filterable": true,
      "sortable": true
    },
    "accountId": {
      "$id": "/properties/accountId",
      "type": "integer",
      "title": "Cuenta Padre",
      "table": "account",
      "tableAlias": "parentAccount",
      "route": "account/findLikeName",
      "element": "autocomplete",
      "filterable": true,
      "elementOptions": {
        "primary": "name"
      },
      "width": 200
    }
  },
  "belongsTo": [
    "account"
  ],
  "actions": [
    "Crear",
    "Editar",
    "Marcar",
    {
      "title": "Importar",
      "type": "modal",
      "size": "md"
    }
  ],
  "layout": [
    {
      "title": "Informaci√≥n",
      "cols": 4,
      "fields": [
        "name",
        "codigo",
        "accountId",
        "normalCredit",
        "normalDebit"
      ]
    }
  ],
  "table": {
    "sum": [],
    "fields": [
      "codigo",
      "name",
      "balance",
      "normalCredit",
      "normalDebit",
      "balancePendiente",
      "accountId"
    ]
  },
  "batch": [
    "accountId"
  ]
}