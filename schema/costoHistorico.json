{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Costo Historico",
  "key": "costoHistorico",
  "limit": 100,
  "sort": "id",
  "maxLimit": 1000,
  "namespace": "auditoria",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id",
      "width": 50
    },
    "activo": {
      "$id": "/properties/activo",
      "type": "boolean",
      "title": "Activo",
      "formatter": "BooleanFormatter",
      "default": true
    },
    "costo": {
      "$id": "/properties/costo",
      "type": "number",
      "title": "Costo"
    },
    "costoAnterior": {
      "$id": "/properties/costoAnterior",
      "type": "number",
      "title": "CostoAnterior",
      "width": 150
    },
    "costoIngresado": {
      "$id": "/properties/costoIngresado",
      "type": "number",
      "title": "Costo Ingresado",
      "width": 150
    },
    "inventarioInicial": {
      "$id": "/properties/inventarioInicial",
      "type": "number",
      "title": "Inventario inicial",
      "width": 150
    },
    "inventarioFinal": {
      "$id": "/properties/inventarioFinal",
      "type": "number",
      "title": "Inventario final",
      "width": 150
    },
    "inventarioIngresado": {
      "$id": "/properties/inventarioIngresado",
      "type": "number",
      "title": "Inventario ingresado",
      "width": 150
    },
    "productoId": {
      "$id": "/properties/productoId",
      "type": "integer",
      "title": "Producto",
      "table": "producto",
      "metadataType": "producto",
      "filterable": true,
      "route": "producto/findLikeName",
      "element": "autocomplete",
      "width": 350,
      "elementOptions": {
        "primary": "name"
      }
    },
    "marca": {
      "$id": "/properties/marca",
      "type": "string",
      "title": "Marca",
      "filterable": true,
      "sortable": true,
      "select": "producto.marca",
      "width": 100
    },
    "grupo": {
      "$id": "/properties/grupo",
      "type": "string",
      "title": "Grupo",
      "filterable": true,
      "sortable": true,
      "select": "producto.grupo",
      "width": 190
    },
    "createdBy": {
      "$id": "/properties/createdBy",
      "type": "string",
      "title": "Creado por"
    },
    "updatedBy": {
      "$id": "/properties/updatedBy",
      "type": "string",
      "title": "Actualizado por"
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado",
      "filterable": true,
      "filterRenderer": "DateTimeFilter",
      "sortable": true,
      "width": 180
    },
    "updatedAt": {
      "$id": "/properties/updatedAt",
      "type": "string",
      "title": "Actualizado"
    }
  },
  "belongsTo": ["producto"],
  "required": [],
  "table": {
    "sum": [],
    "fields": [
      "id",
      "productoId",
      "marca",
      "grupo",
      "costo",
      "costoAnterior",
      "costoIngresado",
      "inventarioInicial",
      "inventarioFinal",
      "inventarioIngresado",
      "createdAt"
    ]
  }
}
