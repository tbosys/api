{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Logs",
  "key": "log",
  "namespace": "adminitracion",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "route": {
      "$id": "/properties/route",
      "type": "string",
      "title": "route",
      "filterable": true,
      "sortable": true,
      "width": 200
    },
    "usuarioNombre": {
      "$id": "/properties/usuarioNombre",
      "type": "string",
      "title": "usuarioNombre",
      "filterable": true
    },
    "evento": {
      "$id": "/properties/evento",
      "type": "string",
      "title": "evento",
      "element": "bigJSON"
    },
    "error": {
      "$id": "/properties/error",
      "type": "string",
      "title": "error",
      "element": "bigJSON"
    },
    "remainingTimeInMillis": {
      "$id": "/properties/remainingTimeInMillis",
      "type": "number",
      "title": "remainingTimeInMillis"
    },
    "functionName": {
      "$id": "/properties/functionName",
      "type": "string",
      "title": "functionName",
      "filterable": true
    },
    "logGroupName": {
      "$id": "/properties/logGroupName",
      "type": "string",
      "title": "logGroupName",
      "filterable": true
    },
    "logStreamName": {
      "$id": "/properties/logStreamName",
      "type": "string",
      "title": "logStreamName",
      "filterable": true
    },
    "status": {
      "$id": "/properties/status",
      "type": "string",
      "title": "Status",
      "filterable": true,
      "sortable": true
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado",
      "filterable": true,
      "filterRenderer": "DateTimeFilter",
      "sortable": true,
      "width": 180
    }
  },
  "belongsTo": [
    "usuario"
  ],
  "actions": [
    "Activar",
    "Desactivar"
  ],
  "layout": [
    {
      "title": "Informaci√≥n",
      "cols": 4,
      "fields": [
        "functionName",
        "usuarioNombre",
        "logStreamName",
        "logGroupName"
      ]
    },
    {
      "title": "Detalles",
      "cols": 12,
      "fields": [
        "evento"
      ]
    },
    {
      "title": "Totales",
      "cols": 12,
      "fields": [
        "error"
      ]
    }
  ],
  "table": {
    "fields": [
      "createdAt",
      "status",
      "usuarioNombre",
      "route",
      "remainingTimeInMillis",
      "functionName",
      "logGroupName",
      "logStreamName",
      "evento",
      "error"
    ]
  }
}