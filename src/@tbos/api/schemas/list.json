{
  "$id": "http://example.com/example.json",
  "type": "object",
  "restricted": [],
  "menu": false,
  "title": "Filter List",
  "description":"Filters for Reports, ",
  "key": "list",
  "secure": false,
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "Id"
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "Nombre",
      "width": 450
    },

    "sums": {
      "$id": "/properties/sums",
      "type": "string",
      "element": "multiselectDynamic",
      "route": "reporte/queryProperties",
      "queryOnChange": ["table"],
      "isJSON": true,
      "title": "sumas"
    },
    "sorts": {
      "$id": "/properties/sorts",
      "type": "string",
      "element": "multiselectDynamic",
      "route": "reporte/queryProperties",
      "queryOnChange": ["table"],
      "isJSON": true,
      "title": "orden"
    },
    "columns": {
      "$id": "/properties/columns",
      "type": "string",
      "element": "multiselectDynamic",
      "route": "reporte/queryProperties",
      "queryOnChange": ["table"],
      "isJSON": true,
      "title": "Campos"
    },
    "table": {
      "$id": "/properties/table",
      "type": "string",
      "element": "selectDynamic",
      "route": "reporte/queryTables",
      "title": "Table"
    },
    "type": {
      "$id": "/properties/type",
      "type": "string",
      "element": "selectDynamic",
      "route": "reporte/queryTables",
      "title": "Type"
    },
    "filters": {
      "$id": "/properties/filters",
      "type": "string",
      "isJSON": true,
      "title": "Filtros"
    },
    "ownerId": {
      "$id": "/properties/ownerId",
      "type": "integer"
    },
    "groups": {
      "$id": "/properties/groups",
      "type": "string",
      "element": "multiselectDynamic",
      "route": "reporte/queryProperties",
      "queryOnChange": ["table"],
      "isJSON": true,
      "title": "Agrupar"
    },
    "createdBy": {
      "$id": "/properties/createdBy",
      "type": "string",
      "title": "Creado por",
      "filterable": true,
      "sortable": true
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "string",
      "title": "Creado",
      "filterable": true,
      "sortable": true,
      "formatter": "DateTimeFormatter"
    }
  },

  "belongsTo": []
}
