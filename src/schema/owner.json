{
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "Owner",
  "key": "owner",
  "restricted": [],
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer"
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "default": "",
      "minLength": 2
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "default": "",
      "minLength": 3
    },
    "active": {
      "$id": "/properties/active",
      "type": "boolean",
      "default": true
    }
  },
  "belongsInMany": ["profileOwner"],
  "required": ["name", "email", "active"]
}
